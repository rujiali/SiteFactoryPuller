{
    "name": "rujiali/acquia-site-factory-cli",
    "description": "An Acquia site factory command line tool.",
    "keywords": ["Acquia site factory", "RESTFUL API", "Command line"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Li",
            "email": "joshua@lirujia.com"
        }
    ],
    "type": "project",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/console": "^3.3",
        "symfony/yaml": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/config": "^3.3"
    },
    "bin": ["bin/AcquiaSiteFactoryCli"],
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "squizlabs/php_codesniffer": "3.*",
        "escapestudios/symfony2-coding-standard": "3.x-dev"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "cs": "./vendor/bin/phpcs --standard=Symfony ./src/",
        "cbf": "./vendor/bin/phpcbf --standard=Symfony ./src/"
    }
}
